# Core Framework Configuration
# Docs: docs/02-settings.md
#
# Note: .env and src/settings.py are the primary configuration sources.
# This file provides fallback values for CLI commands.

[core]
# Database
database_url = "sqlite+aiosqlite:///./app.db"

# Migrations
migrations_dir = "migrations"
app_label = "app"

# Models discovery (optional - auto-discovered by default)
models_module = "src.apps.models"

# Application
app_module = "src.main:app"
