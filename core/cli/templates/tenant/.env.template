# Environment variables - Multi-tenant

# Application
APP_NAME={{project_name}}
ENVIRONMENT=development
DEBUG=true
SECRET_KEY=change-me-in-production

# PostgreSQL (required for multi-tenant)
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost/{{project_name}}

# Tenant settings
TENANT_HEADER=X-Tenant-ID
TENANT_ID_TYPE=uuid
