# Environment variables - Background workers

# Application
APP_NAME={{project_name}}
ENVIRONMENT=development
DEBUG=true
SECRET_KEY=change-me-in-production

# PostgreSQL
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost/{{project_name}}

# Redis (task queue)
REDIS_URL=redis://localhost:6379/0

# Worker settings
WORKER_CONCURRENCY=4
TASK_RETRY_MAX=3
TASK_RETRY_DELAY=60
